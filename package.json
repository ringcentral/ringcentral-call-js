{
  "name": "ringcentral-call",
  "version": "0.2.19",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ringcentral/ringcentral-call-js.git"
  },
  "homepage": "https://github.com/ringcentral/ringcentral-call-js#readme",
  "scripts": {
    "prepublish": "tsc && webpack",
    "tsc": "tsc",
    "build": "webpack",
    "watch": "webpack --watch",
    "dev-server": "http-server -p 8080",
    "start": "yarn dev-server",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "test:showCoverage": "cat ./coverage/lcov.info | coveralls"
  },
  "peerDependencies": {
    "ringcentral-call-control": "^0.2.8",
    "ringcentral-web-phone": "^0.8.10"
  },
  "devDependencies": {
    "@ringcentral/sdk": "^4.7.3",
    "@ringcentral/subscriptions": "^4.7.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/pubnub": "4.0.2",
    "coveralls": "^3.1.1",
    "http-server": "^14.1.0",
    "jest": "^27.5.1",
    "ringcentral-call-control": "^0.2.8",
    "ringcentral-web-phone": "^0.8.10",
    "ts-jest": "^27.1.4",
    "ts-loader": "^5.3.3",
    "tslint": "^6.1.1",
    "typescript": "^3.8.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [
      "<rootDir>/test/support/setup.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/support/jest.setup.js"
    ],
    "reporters": [
      "default"
    ],
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  }
}
